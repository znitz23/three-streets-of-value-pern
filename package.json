{
  "name": "poker_app_data",
  "version": "1.0.0",
  "description": "A simple boilerplate for the PERN stack using Vite.js. It includes Node-Postgres v8.8, Express, React 18, React Router DOM v6.6, Morgan, Axios, jsonwebtoken, bcrypt, dotenv, and Nodemon. ",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "start:dev": "(npm run dev --prefix client) & nodemon server/index.js",
    "seed": "node server/db/seed.js",
    "seed:dev": "nodemon server/db/seed.js",
    "build": "(npm run build --prefix client)"
  },
  "author": "Zach-Nitzkin",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-styled-components": "^2.1.4",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "react-router-dom": "^6.14.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
